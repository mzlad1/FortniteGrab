{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://fortnitegrab.onrender.com/api/:path*"
    }
  ]
}
